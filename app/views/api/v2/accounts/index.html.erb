<p id="notice"><%= notice %></p>

<h1>Listing Api V2 Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Uuid</th>
      <th>Hruid</th>
      <th>Id soce</th>
      <th>Enabled</th>
      <th>Password</th>
      <th>Lastname</th>
      <th>Firstname</th>
      <th>Birthname</th>
      <th>Birth firstname</th>
      <th>Email</th>
      <th>Gapps email</th>
      <th>Password</th>
      <th>Birthdate</th>
      <th>Deathdate</th>
      <th>Gender</th>
      <th>Is gadz</th>
      <th>Is student</th>
      <th>School</th>
      <th>Is alumni</th>
      <th>Date entree ecole</th>
      <th>Date sortie ecole</th>
      <th>Ecole entree</th>
      <th>Buque texte</th>
      <th>Buque zaloeil</th>
      <th>Gadz fams</th>
      <th>Gadz fams zaloeil</th>
      <th>Gadz proms principale</th>
      <th>Gadz proms secondaire</th>
      <th>Avatar url</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v2_accounts.each do |api_v2_account| %>
      <tr>
        <td><%= api_v2_account.uuid %></td>
        <td><%= api_v2_account.hruid %></td>
        <td><%= api_v2_account.id_soce %></td>
        <td><%= api_v2_account.enabled %></td>
        <td><%= api_v2_account.password %></td>
        <td><%= api_v2_account.lastname %></td>
        <td><%= api_v2_account.firstname %></td>
        <td><%= api_v2_account.birthname %></td>
        <td><%= api_v2_account.birth_firstname %></td>
        <td><%= api_v2_account.email %></td>
        <td><%= api_v2_account.gapps_email %></td>
        <td><%= api_v2_account.password %></td>
        <td><%= api_v2_account.birthdate %></td>
        <td><%= api_v2_account.deathdate %></td>
        <td><%= api_v2_account.gender %></td>
        <td><%= api_v2_account.is_gadz %></td>
        <td><%= api_v2_account.is_student %></td>
        <td><%= api_v2_account.school_id %></td>
        <td><%= api_v2_account.is_alumni %></td>
        <td><%= api_v2_account.date_entree_ecole %></td>
        <td><%= api_v2_account.date_sortie_ecole %></td>
        <td><%= api_v2_account.ecole_entree %></td>
        <td><%= api_v2_account.buque_texte %></td>
        <td><%= api_v2_account.buque_zaloeil %></td>
        <td><%= api_v2_account.gadz_fams %></td>
        <td><%= api_v2_account.gadz_fams_zaloeil %></td>
        <td><%= api_v2_account.gadz_proms_principale %></td>
        <td><%= api_v2_account.gadz_proms_secondaire %></td>
        <td><%= api_v2_account.avatar_url %></td>
        <td><%= api_v2_account.description %></td>
        <td><%= link_to 'Show', api_v2_account_path(api_v2_account) %></td>
        <td><%= link_to 'Edit', edit_api_v2_account_path(api_v2_account) %></td>
        <td><%= link_to 'Destroy', api_v2_account_path(api_v2_account) , method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

